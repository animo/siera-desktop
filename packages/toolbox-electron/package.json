{
  "name": "@animo/toolbox-electron",
  "productName": "toolbox-electron",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "clean": "rimraf -rf ./out ./.webpack",
    "build": "yarn clean && yarn package"
  },
  "keywords": [],
  "license": "MIT",
  "private": true,
  "author": {
    "url": "https://animo.id",
    "name": "Animo Solutions",
    "email": "contact@animo.id"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "toolbox-electron",
        "executableName": "toolbox-electron"
      },
      "electronRebuildConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "toolbox-electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devServer": {
              "liveReload": false,
              "hot": true
            },
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.67",
    "@electron-forge/plugin-webpack": "6.0.0-beta.65",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "@types/indy-sdk": "^1.16.21",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.0.0",
    "electron": "19.0.11",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "patch-package": "^6.4.7",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@aries-framework/core": "^0.2.4",
    "@aries-framework/node": "^0.2.3",
    "@aries-framework/react-hooks": "^0.3.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "electron-squirrel-startup": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "indy-sdk": "^1.16.0-dev-1655",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
